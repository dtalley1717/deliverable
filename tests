import junit.framework.Test;
import junit.framework.TestSuite;

public class Tests {

  public static Test suite() {
    TestSuite suite = new TestSuite();
    suite.addTestSuite(Panther Central.class);
    suite.addTestSuite(Student Affairs.class);
    suite.addTestSuite(Sports.class);
    suite.addTestSuite(Apply.class);
    suite.addTestSuite(Technology Services.class);
    return suite;
  }

  public static void main(String[] args) {
    junit.textui.TestRunner.run(suite());
  }
}
